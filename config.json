model:
    arch: frrnB
data:
    dataset: labelmefacade
    train_split: train
    val_split: train
    img_rows: 512
    img_cols: 683
    path: /home/dunn/Downloads/labelmefacade-master-2
training:
    train_iters: 85000
    batch_size: 1
    val_interval: 500
    print_interval: 25
    optimizer:
        lr: 1.0e-4
        name: adam
    lr_schedule:
        name: poly_lr
        max_iter: 85000
    momentum: 0.99
    weight_decay: 0.0005
    resume: frrnB_cityscapes_best_model.pkl
    visdom: False
    n_workers: 2
    loss:
        name: cross_entropy
